{
    "name": "madj2k/t3-ajax-api",
    "type": "typo3-cms-extension",
    "description": "AJAX-API for usage with TYPO3, based on simple ViewHelpers and some JS",
    "homepage": "https://www.steffenkroggel.dede",
    "license": ["GPL-2.0+"],
    "keywords": ["TYPO3 CMS", "AJAX", "API"],
    "support": {
        "email": "developer@steffenkroggel.de"
    },
    "authors": [
        {
            "name": "Steffen Kroggel",
            "email": "developer@steffenkroggel.de",
            "homepage": "http://www.steffenkroggel.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=7.4",
		"ext-json": "*",
		"ext-dom": "*",
		"masterminds/html5": "^2.0",
        "typo3/cms-core": "~9.5.0",
		"madj2k/t3-core-extended": "~9.5.0"
    },
    "autoload": {
        "psr-4": {
            "Madj2k\\AjaxApi\\": "Classes/"
        }
    },
    "extra": {
        "typo3/cms": {
            "extension-key": "ajax_api"
        }
    }
}
